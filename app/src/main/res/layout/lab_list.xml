<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    
   <TextView 
        android:id="@+id/label_sr_no"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:text="sr.no"
        android:textSize="13sp"/>
   
   <TextView 
        android:id="@+id/value_sr_no_lab"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        
        android:textSize="13sp"/>
   
   
   <TextView 
        android:id="@+id/label_lab_setup"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:text="Lab Setup"
        android:layout_marginTop="5dp"
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
   <TextView 
        android:id="@+id/value_lab_setup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        
        
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
   
   <TextView 
        android:id="@+id/label_lab_fan"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:text="Lab Fan"
        android:layout_marginTop="5dp"
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
   <TextView 
        android:id="@+id/value_lab_fan"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
       
        
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
   
   <TextView 
        android:id="@+id/label_lab_stand"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:text="Lab Stand"
        android:layout_marginTop="5dp"
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
   <TextView 
        android:id="@+id/value_lab_stand"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        
        
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
   
   
   
   <TextView 
        android:id="@+id/label_lab_kit"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:text="Lab kit"
        android:layout_marginTop="5dp"
        android:layout_below="@id/audit_name"
        android:textSize="13sp"/>
   
